<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="phoneUserBean" class="dependencyInjectionBasic.PhoneUser">
		<constructor-arg value="10" type="int"></constructor-arg>
	</bean>
	<bean id="phoneUserBean2" class="dependencyInjectionBasic.PhoneUser">
		<constructor-arg value="Constructor Arg Name" type="String"></constructor-arg>
	</bean>
	<bean id="phoneUserBean3" class="dependencyInjectionBasic.PhoneUser">
		<constructor-arg value="Constructor Arg Name and Setter for Id(123)"
			type="String"></constructor-arg>
		<property name="userId" value="123"></property>
	</bean>
	<bean id="phoneUserBean4" class="dependencyInjectionBasic.PhoneUser">
		<constructor-arg value="Original Constructor Arg Name"
			type="String"></constructor-arg>
		<property name="userName" value="Setter Name"></property>
	</bean>
	<bean id="phoneUserBean5" class="dependencyInjectionBasic.PhoneUser">
	</bean>

</beans>  