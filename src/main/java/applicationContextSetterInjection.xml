<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="phoneUserBean" class="springBasic.PhoneUser">
		<property name="userName" value="User from BeanFactory"></property>
		<property name="userId" value="1"></property>
	</bean>
	<bean id="phoneUserBean2" class="springBasic.PhoneUser">
		<property name="userName" value="User from ApplicationContext"></property>
		<property name="userId" value="2"></property>
	</bean>

</beans>  